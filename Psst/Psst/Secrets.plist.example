<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Google Calendar OAuth Configuration -->
	<!-- Get these from: https://console.cloud.google.com/ → APIs & Services → Credentials -->

	<!-- iOS OAuth 2.0 Client ID (NOT the web client ID from Firebase) -->
	<key>GOOGLE_CLIENT_ID</key>
	<string>YOUR_CLIENT_ID_HERE.apps.googleusercontent.com</string>

	<!-- Client Secret (empty for iOS clients - they don't use secrets) -->
	<key>GOOGLE_CLIENT_SECRET</key>
	<string></string>

	<!-- Redirect URI (reverse client ID format) -->
	<key>GOOGLE_REDIRECT_URI</key>
	<string>com.googleusercontent.apps.YOUR_CLIENT_ID_HERE:/oauth2callback</string>

	<!-- OAuth Scopes (space-separated) -->
	<key>GOOGLE_SCOPE</key>
	<string>https://www.googleapis.com/auth/calendar.events email</string>
</dict>
</plist>
